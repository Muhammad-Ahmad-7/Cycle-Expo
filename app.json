{
  "expo": {
    "name": "Cycle-Eccomerce",
    "slug": "Cycle-Eccomerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e1f28"
      }
    },
    "splash": {
      "backgroundColor": "#1e1f28",
      "resizeMode": "contain",
      "image": "./assets/splash.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#1e1f28"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
